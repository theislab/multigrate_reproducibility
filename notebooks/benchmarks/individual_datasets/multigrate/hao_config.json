{
    "model": {
        "name": "MultiVAE_PoE_cond",
        "params": {
            "adatas": [
                ["multigrate/data/hao-2020/adata1.h5ad", "multigrate/data/hao-2020/adata2.h5ad"],
                ["multigrate/data/hao-2020/cite1.h5ad", "multigrate/data/hao-2020/cite2.h5ad"]
            ],
            "names": [["rna1", "rna2"], ["cite1", "cite2"]],
            "pair_groups": [["pair1", "pair2"], ["pair1", "pair2"]],
            "condition": true,
            "recon_coef": 1,
            "kl_coef": 0.00001,
            "integ_coef": 0.1,
            "cycle_coef": 0,
            "z_dim": 15,
            "h_dim": 32,
            "hiddens": [[], []],
            "layers": [["count", "count"], [null, null]],
            "output_activations": ["linear", "linear"],
            "losses": ["nb", "mse"],
            "shared_hiddens": [],
            "dropout": 0.2
        },
        "train": {
            "n_iters": 150000,
            "lr": 3e-4,
            "batch_size": 64,
            "validate_every": 15000,
            "kl_anneal_iters": 75000,
            "verbose": 1
        }
    },
    "experiment": {
        "seed": 42,
        "pair-split": 1,
        "batch": true,
        "batch_key": "study",
        "calculate_metrics": true,
        "impute": false
    }
}
